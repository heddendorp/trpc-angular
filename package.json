{
  "name": "trpc-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "yarn workspaces foreach --all -pt run build",
    "format": "prettier . --write"
  },
  "workspaces": [
    "projects/*"
  ],
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/router": "^20.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.0",
    "@angular/cli": "^20.1.0",
    "@angular/compiler-cli": "^20.1.0",
    "@tanstack/angular-query-experimental": "^5.83.0",
    "@trpc/client": "^11.4.3",
    "@trpc/server": "^11.4.3",
    "jsdom": "^26.1.0",
    "ng-packagr": "^20.1.0",
    "prettier": "3.6.2",
    "typescript": "~5.8.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "yarn@4.9.2"
}
